import java.util.Scanner;
public class Main {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        System.out.println("enter the number : ");
        int n = input.nextInt();
        int ans = 0;
        while(n>0){
            int rem = n%10; //344533
            
            ans = (ans*10)+rem;
            n=n/10;
            
        }
        System.out.println("the reversed number is "+ans);
            
        }
    }
