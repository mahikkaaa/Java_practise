import java.util.Scanner;
public class Main {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        System.out.println("enter the number : ");
        int n = input.nextInt();
        int count=0;
        while(n>0){
            int rem = n%10; //344533
            if(rem==3){
                count++;
            }
            n=n/10;
    System.out.println(count);
            
        }
    }}
